<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="black_15">#26000000</color>
    <color name="black_26">#42000000</color>
    <color name="black_54">#8A000000</color>
    <color name="black_87">#B3000000</color>
    <color name="white">#FFFFFF</color>
    <style name="SpinnerBase">
        <item name="android:singleLine">true</item>
        <item name="android:minHeight">48dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <declare-styleable name="SpinnerExtended">
        <attr format="string" name="spinnerHint"/>
        <attr format="color" name="spinnerHintColor"/>
        <attr format="color" name="spinnerSelectedItemColor"/>
        <attr format="reference" name="spinnerSelectedItemTextStyle"/>
        <attr format="boolean" name="spinnerDialogMode"/>
        <attr format="integer" name="spinnerSelectedItemLayout"/>
    </declare-styleable>
</resources>