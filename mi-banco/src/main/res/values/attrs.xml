<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CoverFlow">
        <attr name="imageWidth" format="dimension" />
        <attr name="imageHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CustomFastScrollView">
        <attr name="overlayWidth" format="dimension" />
        <attr name="overlayHeight" format="dimension" />
        <attr name="overlayTextSize" format="dimension" />
        <attr name="overlayScrollThumbWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="WheelView">
        <attr name="visibleItems" format="integer" />
        <attr name="itemHeight" format="integer" />
        <attr name="centerDrawable" format="reference" />
        <attr name="shaddowHeight" format="integer" />
    </declare-styleable>
    <declare-styleable name="AspectRatioImageView">
        <attr name="minScreenHeight" format="dimension" />
        <attr name="fixedHeight" format="dimension" />
    </declare-styleable>

</resources>